<?xml version="1.0" encoding="UTF-8"?>
<dictionary>
    <message name="HELP_CONTROL_TITLE1" value="Переміщення вліво/вправо"/>
    <message name="HELP_CONTROL_DESCR1" >
        <![CDATA[<b>Стрілки вліво/вправо</b> на клавіатурі клавіші <b>A/D</b>]]>
    </message>

    <message name="HELP_CONTROL_TITLE2" value="Стрибок вперед"/>
    <message name="HELP_CONTROL_DESCR2">
        <![CDATA[Клавіша <b>Enter</b> або клавіша <b>F</b>]]>
    </message>

    <message name="HELP_CONTROL_TITLE3" value="Стрибок назад"/>
    <message name="HELP_CONTROL_DESCR3">
        <![CDATA[Клавіша <b>Backspace</b> або клавіша <b>R</b>]]>
    </message>

    <message name="HELP_CONTROL_TITLE4" value="Переміщення прицілу"/>
    <message name="HELP_CONTROL_DESCR4">
        <![CDATA[<b>Стрілки вгору/вниз</b> на клавіатурі клавіші <b>W/S</b>]]>
    </message>

    <message name="HELP_CONTROL_TITLE5" value="Стріляти"/>
    <message name="HELP_CONTROL_DESCR5">
        <![CDATA[Клавіша <b>пробіл</b>. Для зміни сили пострілу утримуйте клавішу пробіл]]>
    </message>

    <message name="HELP_CONTROL_TITLE6" value="Вибір зброї в рюкзаку"/>
    <message name="HELP_CONTROL_DESCR6">
        <![CDATA[<b>Q/I</b> - відкрити, <b>A/W/S/D</b> - переміщення, <b>пробіл/E</b> - вибрати]]>
    </message>

    <message name="HELP_CONTROL_TITLE7" value="Завдання таймера для гранати"/>
    <message name="HELP_CONTROL_DESCR7">
        <![CDATA[Використовуйте <b>клавіши 1-5</b>]]>
    </message>
</dictionary>