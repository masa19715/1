<dictionary>

    <!-- format description
    * d, dd  - day of month
    * ddd - short day of week
    * dddd - day of week
    * m, mm - month number
    * mmm - short month name
    * mmmm - month name
    * y - year (valid y, yy, yyyy)
    * h, hh - hour
    * M, MM - minutes
    * s, ss - seconds
    *
    * example
    * ddd, mm.d.yyyy hh:MM
    * Mon, 06.1.2011 22:30
    */
    -->
    <message name="date_format" value="mm.dd.yyyy"/>
    <message name="time_format" value="hh.MM"/>
    <message name="date_full_format" value="dddd, mmmm d yyyy"/>

